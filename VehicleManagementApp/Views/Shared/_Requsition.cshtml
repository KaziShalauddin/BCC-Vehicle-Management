<!doctype html>
<html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>VehicleManagement</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="manifest" href="site.webmanifest">
    <link rel="apple-touch-icon" href="icon.png">
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/slicknav.css">
    <link rel="stylesheet" href="~/css/animate.css">
    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/css/normalize.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link href="~/Content/DataTables/css/dataTables.jqueryui.min.css" rel="stylesheet" />
    <link href="~/css/jquery.datetimepicker.min.css" rel="stylesheet" />
    <script src="~/js/vendor/modernizr-3.5.0.min.js"></script>
</head>
<body>

    <!-- header start here -->
    <section>
        <nav class="vagigation navbar navbar-inverse">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <div id="navbar" class="collapse navbar-collapse">
                    <ul>
                        <li>@Html.ActionLink("Home", "Index", "Manager")</li>
                    </ul>
                </div><!--/.nav-collapse -->
            </div>
        </nav>
    </section>
    <!-- header end here -->

<div class="container">
    <div class="row">
        <div class="col-md-2">
            <div class="requsition_menu">
                <ul>
                    <li>@Html.ActionLink("New", "New", "Manager")</li>
                    <li>@Html.ActionLink("On Progress", "OnProgress", "Manager")</li>
                    <li>@Html.ActionLink("Assign", "Index", "Manager")</li>
                    <li>@Html.ActionLink("Waiting", "Index", "Manager")</li>
                    <li>@Html.ActionLink("Complete", "Index", "Manager")</li>
                    <li>@Html.ActionLink("Cancle", "Index", "Manager")</li>
                </ul>
            </div>
        </div>
        <div class="col-md-10">
            <div class="welcome">
                @RenderBody()
            </div>
        </div>
    </div>
</div>
<div class="footer-menu">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="theme text-centre">
                    <p>All right Reserved @DateTime.Now.Year</p>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/scripts/jquery-2.2.4.min.js"></script>
<script src="~/scripts/jquery.validate.js"></script>
<script src="~/scripts/jquery.validate.unobtrusive.js"></script>
<script src="~/scripts/DataTables/jquery.dataTables.min.js"></script>
<script src="~/scripts/jquery.datetimepicker.full.min.js"></script>
<script src="~/js/plugins.js"></script>
<script src="~/js/bootstrap.min.js"></script>
<script src="~/js/jquery.slicknav.min.js"></script>
<script src="~/js/owl.carousel.min.js"></script>
<script src="~/js/wow.min.js"></script>
<script src="~/js/jquery.fitvids.js"></script>
<script src="~/js/main.js"></script>


@RenderSection("scripts", required: false)

<script type="text/javascript">
    $(document).ready(function() {
        $('#myTable').DataTable();
    });
</script>
</body>
</html>

